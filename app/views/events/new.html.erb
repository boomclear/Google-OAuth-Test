  <%= form_with url: "/create" do |f| %>
  <%= f.label :title %>
        <%= f.text_field :title %>
        <%= f.label :description %>
        <%= f.text_area :description %>
        <%= f.label :datetime %>
        <%= f.datetime_local_field :date_time %>
        <%= f.label :endtime %>
        <%= f.datetime_local_field :end_time %>
    <%= f.submit "Create Event" %>
  <% end %>